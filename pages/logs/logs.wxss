.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
.text-title {
  font-size: 30rpx;
  color: #353535;
  line-height: 2em;
  display: flex;
}
.text-price {
  font-size: 30rpx;
  color: #353535;
  line-height: 2em;
  display: flex;
}
.text-haulage {
  font-size: 30rpx;
  color: #353535;
  line-height: 2em;
  display: flex;
}

.container-bottom {
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 80rpx;
}

.haulage-btn {
  flex: 1;
}

.add-btn {
  flex: 2;
  background-color: #353535;
}

.buy-btn {
  flex: 2;
  background-color: #653553;
}


